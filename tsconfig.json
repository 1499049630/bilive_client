{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "build",
    "removeComments": true,
    "noImplicitAny": false,
    "sourceMap": false,
    "declaration": false,
    "strictNullChecks": true
  },
  "exclude": [
    "node_modules"
  ]
}